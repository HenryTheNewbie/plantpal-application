<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomBottomNavView" parent="Widget.Design.BottomNavigationView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/nav_bar_background</item>
        <item name="android:elevation">8dp</item>
    </style>
    <style name="CustomBottomNavTextAppearance" parent="TextAppearance.AppCompat.Medium">
        <item name="fontFamily">@font/agrandir_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#FF000000</item>
    </style>
    <style name="CircleImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="CustomImageCorner">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>
</resources>